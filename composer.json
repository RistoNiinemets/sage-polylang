{
  "name": "generoi/sage-polylang",
  "type": "theme-plugin",
  "license": "MIT",
  "description": "",
  "homepage": "https://github.com/generoi/sage-polylang",
  "authors": [
    {
      "name": "Oskar Schöldström",
      "email": "public@oxy.fi"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/generoi/sage-polylang/issues"
  },
  "autoload": {
    "psr-4": {
      "Genero\\Sage\\Polylang\\": "src/"
    }
  },
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~3.0"
  },
  "extra": {
    "acorn": {
      "providers": [
        "Genero\\Sage\\Polylang\\Providers\\PolylangServiceProvider",
        "Genero\\Sage\\Polylang\\Providers\\TranslationFinderServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": [
      "phpcs --ignore=vendor --extensions=php --standard=PSR2 ."
    ]
  },
  "archive" : {
    "exclude": [
      ".gitignore"
    ]
  }
}
